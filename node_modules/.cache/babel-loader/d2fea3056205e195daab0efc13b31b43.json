AAA,EAAA;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,YAAA,EAAA,KAAA,IAAA;AACA,QAAA,KAAA,CAAA,MAAA,CAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,IAAA;AACA,QAAA,CAAA,CAAA,MAAA,CAAA,SAAA,GAAA,aAAA;AACA;AACA;AA1BA,GAVA;AAuCA,EAAA,KAAA,EAAA,EAvCA;AAyCA,EAAA,OAzCA,qBAyCA;AAAA;;AACA,IAAA,KAAA,CACA,2GADA,CAAA,CAGA,IAHA,CAGA,UAAA,OAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,EAAA;AACA,KALA,EAMA,IANA,CAMA,UAAA,IAAA,EAAA;AACA,MAAA,KAAA,CAAA,SAAA,GAAA,IAAA,CAAA,OAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA,EAAA,KAAA,CAAA,SAAA;AACA,KATA;AAUA,GApDA;AAsDA,EAAA,OAtDA,qBAsDA;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,YAAA,KAAA,IAAA,EAAA;AACA,QAAA,CAAA,GAAA,YAAA,CAAA,OAAA,CAAA,YAAA,EAAA,KAAA,CAAA,GAAA,CAAA;;AACA,SAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,CAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,WAAA,IAAA,CAAA,IAAA,CAAA,MAAA,CAAA,CAAA,CAAA,CAAA,CAAA,CAAA;AACA;;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,KAAA,IAAA;AAEA,SAAA,QAAA,GAAA,IAAA,oBAAA,CACA,UAAA,CAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,KAHA,CAAA;AAKA,IAAA,QAAA,CAAA,OAAA,CAAA,QAAA,CAAA,gBAAA,CAAA,KAAA,CAAA;AACA,GApEA;AAsEA,EAAA,OAtEA,qBAsEA;AAAA;;AACA,SAAA,SAAA,CAAA,OAAA,CAAA,UAAA,CAAA,EAAA;AACA,UAAA,YAAA,CAAA,OAAA,CAAA,YAAA,KAAA,IAAA,EAAA;;AACA,UAAA,YAAA,CAAA,OAAA,CAAA,YAAA,EAAA,QAAA,CAAA,CAAA,CAAA,EAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA,EAAA,EAAA,CAAA,EAAA,SAAA,GAAA,aAAA;AACA;AACA,KALA;AAMA,GA7EA;AA8EA,EAAA,aA9EA,2BA8EA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,QAAA,EAAA,KAAA,SAAA;AACA;AAhFA,CAAA","sourcesContent":["<template>\r\n  <div class=\"top-movie\">\r\n    <h2 v-show=\"!this.$store.state.showDetail\">熱門電影</h2>\r\n    <div\r\n      class=\"movie-container\"\r\n      v-show=\"!this.$store.state.showDetail\"\r\n    >\r\n      <div\r\n        class=\"movie\"\r\n        :id=\"d.id\"\r\n        v-for=\"(d, index) in movieData\"\r\n        :key=\"index\"\r\n      >\r\n        <div class=\"movie-img\">\r\n          <img :src=\"imgPath + d.poster_path\" @click=\"detail(d, $event)\" />\r\n          <div class=\"love\" @click=\"loveCheck(d, $event)\" :ref=\"d.id\">❤</div>\r\n        </div>\r\n        <h1>{{ d.original_title }}</h1>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"movieList\",\r\n  data() {\r\n    return {\r\n      movieData: [],\r\n      imgPath: \"https://image.tmdb.org/t/p/w500\",\r\n      love: [],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    detail(d, e) {\r\n      this.$store.commit(\"change\", true);\r\n      this.$router.push({\r\n        path: \"/movieDetail\",\r\n        query: {\r\n          id: d.id,\r\n        },\r\n      });\r\n    },\r\n    loveCheck(d, e) {\r\n      if (this.love.includes(d.id)) {\r\n        this.love = this.love.filter((element) => {\r\n          return element != d.id;\r\n        });\r\n        localStorage.setItem(\"love-movie\", this.love);\r\n        alert(\"從最愛移除\");\r\n        console.log(this.love);\r\n        e.target.className = \"love\";\r\n      } else {\r\n        this.love.push(d.id);\r\n        localStorage.setItem(\"love-movie\", this.love);\r\n        alert(\"加入最愛\");\r\n        console.log(this.love);\r\n        e.target.className = \"love-active\";\r\n      }\r\n    },\r\n    \r\n  },\r\n  watch: {},\r\n\r\n  created() {\r\n    fetch(\r\n      \"https://api.themoviedb.org/3/movie/popular?api_key=e147528034b3b1192f389af6460b3ad9&language=zh-TW&page=1\"\r\n    )\r\n      .then((respone) => {\r\n        return respone.json();\r\n      })\r\n      .then((data) => {\r\n        this.movieData = data.results;\r\n        console.log(\"電影列表來了\", this.movieData);\r\n      });\r\n  },\r\n\r\n  mounted() {\r\n    if (localStorage.getItem(\"love-movie\") == null) return;\r\n    let x = localStorage.getItem(\"love-movie\").split(\",\");\r\n    for (let i = 0; i < x.length; i++) {\r\n      this.love.push(Number(x[i]));\r\n    }\r\n    console.log(this.love);\r\n    \r\n    this.observer=new IntersectionObserver(\r\n      a=>{\r\n        console.log(a)\r\n      }\r\n    )\r\n    observer.observe(document.querySelectorAll('img'))\r\n  },\r\n\r\n  updated() {\r\n    this.movieData.forEach((e) => {\r\n      if (localStorage.getItem(\"love-movie\") == null) return;\r\n      if (localStorage.getItem(\"love-movie\").includes(e.id)) {\r\n        this.$refs[e.id][0].className = \"love-active\";\r\n      }\r\n    });\r\n  },\r\n  beforeDestroy() {\r\n    console.log(\"電影列表死了\", this.movieData);\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@import url(\"https://fonts.googleapis.com/css2?family=Mohave:ital,wght@1,300&display=swap\");\r\nh2 {\r\n  color: white;\r\n}\r\n\r\nh1 {\r\n  font-family: \"Mohave\", sans-serif;\r\n  letter-spacing: 0.1em;\r\n  width: 250px;\r\n  text-align: center;\r\n}\r\n\r\n.movie-container {\r\n  width: 100%;\r\n  margin: auto;\r\n  display: flex;\r\n  justify-content: center;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.movie {\r\n  width: 350px;\r\n  margin: 15px 5px;\r\n  font-size: 10px;\r\n  color: white;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.movie-img {\r\n  position: relative;\r\n}\r\n\r\n.movie img {\r\n  width: 250px;\r\n  height: 350px;\r\n}\r\n\r\n.love {\r\n  position: absolute;\r\n  bottom: 0;\r\n  right: 0;\r\n  font-size: 2em;\r\n}\r\n\r\n.love-active {\r\n  color: red;\r\n  position: absolute;\r\n  bottom: 0;\r\n  right: 0;\r\n  font-size: 2em;\r\n}\r\n</style>"],"sourceRoot":"src/pages"}]}