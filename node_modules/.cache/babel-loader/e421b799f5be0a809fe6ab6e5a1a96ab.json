age=1\"\r\n    )\r\n      .then((respone) => {\r\n        return respone.json();\r\n      })\r\n      .then((data) => {\r\n        this.movieData = data.results;\r\n        console.log(\"電影列表來了\", this.movieData);\r\n      })\r\n  },\r\n  \r\n  updated(){\r\n    this.movieData.forEach(e=>{\r\n          this.$store.state.loveMovie.forEach(a=>{\r\n            if(e.id==a){\r\n              this.$refs[e.id][0].className='love-active'\r\n            }\r\n          })\r\n        })\r\n    \r\n  },\r\n  beforeDestroy() {\r\n    console.log(\"電影列表死了\", this.movieData);\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@import url(\"https://fonts.googleapis.com/css2?family=Mohave:ital,wght@1,300&display=swap\");\r\nh2 {\r\n  color: white;\r\n}\r\n\r\nh1 {\r\n  font-family: \"Mohave\", sans-serif;\r\n  letter-spacing: 0.1em;\r\n}\r\n\r\n.movie-container {\r\n  width: 100%;\r\n  margin: auto;\r\n  display: flex;\r\n  justify-content: center;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.movie {\r\n  width: 350px;\r\n  margin: 15px 5px;\r\n  font-size: 10px;\r\n  color: white;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.movie-img {\r\n  position: relative;\r\n}\r\n\r\n.movie img {\r\n  width: 250px;\r\n  height: 350px;\r\n}\r\n\r\n.love {\r\n  position: absolute;\r\n  bottom: 0;\r\n  right: 0;\r\n  font-size: 2em;\r\n}\r\n\r\n.love-active {\r\n  color: red;\r\n  position: absolute;\r\n  bottom: 0;\r\n  right: 0;\r\n  font-size: 2em;\r\n}\r\n</style>"],"sourceRoot":"src/pages"}]}